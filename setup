# # # Musikku Music Bot Installer
cgreen = '\033[0;32m'
cyellow = '\033[0;33m'
cpurple = '\033[0;35m'
update() {
    sudo apt - get update && sudo apt - get upgrade - y
}
pprint() {
    printf ""
    "$cpurple $1"
    ""
}
packages() {
    sudo apt - get install python3 - pip ffmpeg - y
}
node() {
    curl - fssL https: //deb.nodesource.com/setup_17.x | sudo -E bash - && sudo apt-get install nodejs -y && npm i -g npm
}
repo() {
    git clone https: //github.com/kenkansaja/Musikku &&  cd Musikku
}
installation() {
    pip3 install - U - r requirements.txt
}
clear
pprint "Selamat datang di Penginstal Pengaturan Musikku Bot\n\n"
pprint "Jika Anda melihat kesalahan selama Proses Instalasi, Silakan merujuk ke file-file ini untuk log:"
pprint "\nUntuk kesalahan node js , Periksa nodelog.txt"
pprint "\nUntuk kesalahan paket pypi, Periksa pypilog.txt"
sleep 5
pprint "\n\nMemulai Pembaruan dan Instalasi, Mohon jangan Tekan sembarang tombol sampai Anda diminta untuk mengetikkan nilai Anda.\n\n"
update & > /dev/null
pprint "Pembaruan Telah Selesai, Sekarang Menginstal Paket yang Diperlukan!\n\n"
packages & > /dev/null
pprint "Paket Berhasil Diinstal!, Menginstal Nodejs dan Npm!\n\n"
node & > nodelog.txt
pprint "Nodejs Berhasil Diinstal!, Sekarang Menginstal Paket Pypi!\n\n"
installation > pypilog.txt
pprint "\n\n\n\n\n\n\n\nMusikku Bot Instalasi selesai!"
sleep 5
clear
pprint "Jika Anda melihat kesalahan pada halaman terakhir, Silakan coba jalankan kembali skrip!"
sleep 5
pprint "\nMasukkan Nilai Anda Di Bawah\n\n\n"
pprint "API ID:  "
read api_id
sleep 1
pprint "\nAPI HASH: "
read api_hash
sleep 1
pprint "\nBOT TOKEN: "
read bot_token
sleep 1
pprint "\nMONGO DB URI: "
read mongo_db
sleep 1
pprint "\n MUST JOIN: "
read join
sleep 1
pprint "\nLOG GROUP ID: "
read logger
sleep 1
pprint "\nPYROGRAM STRING SESSION OF ASSISTANT ACCOUNT: "
read string_session
sleep 1
pprint "\nMUSIC BOT NAME: "
read mbt
pprint "\nOwner Id :"
read ownid
pprint "$cgreen \n\nMemproses vars Anda, Tunggu sebentar!"
sleep 5
if [-f.env];
then
sudo rm.env
fi
echo ""
"API_ID = $api_id
API_HASH = $api_hash
BOT_TOKEN = $bot_token
MONGO_DB_URI = $mongo_db
MUST_JOIN = $join
LOG_GROUP_ID = $logger
MUSIC_BOT_NAME = $mbt
STRING_SESSION = $string_session
OWNER_ID = $ownid ""
" > .env
clear
pprint "\n\n\nVars Anda telah berhasil disimpan!, Terima kasih telah menggunakan Musikku Installer, sekarang Anda dapat melanjutkan dengan memulai bot dengan bash start!"
pprint "\n\n\nIngin lebih banyak vars?"
pprint "\nCheckout config.py dan readme.md di dalam folder config untuk vars tambahan. Anda dapat mengubah semua gambar , Thumbs, mode dan semuanya dari vars. Lihat ke arah mereka\n\n"
